{
  "name": "renting-backend",
  "scripts": {
    "start": "node dist/server",
    "clean": "rm -rf dist",
    "build": "npm -s run clean && npm -s run generate && tsc",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma generate",
    "migrate": "prisma migrate dev --preview-feature",
    "seed": "prisma db seed --preview-feature",
    "generate:nexus": "ts-node --transpile-only src/schemas/schema",
    "dev": "ts-node-dev --no-notify --respawn --transpileOnly src/server",
    "test": "jest"
  },
  "dependencies": {
    "@prisma/client": "~2.17.0",
    "@sentry/node": "^6.1.0",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-context": "^1.0.20",
    "apollo-link-http": "^1.5.17",
    "apollo-server": "^2.19.2",
    "bcrypt": "^5.0.0",
    "bull": "^3.20.1",
    "dayjs": "^1.10.4",
    "dotenv": "^8.2.0",
    "email-templates": "^8.0.3",
    "graphql": "^15.5.0",
    "graphql-middleware": "^6.0.3",
    "graphql-shield": "^7.5.0",
    "i18n": "^0.13.2",
    "jsonwebtoken": "^8.5.1",
    "nexus": "^1.0.0",
    "nexus-plugin-prisma": "^0.31.0",
    "pug": "^3.0.0",
    "stripe": "^8.134.0",
    "ts-node-dev": "1.0.0-pre.50",
    "typescript": "^4.1.5",
    "yup": "^0.31.0",
    "prisma": "~2.17.0",
    "@types/node": "^12.19.4"
  },
  "devDependencies": {
    "@apollo/client": "^3.3.9",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@types/bcrypt": "^3.0.0",
    "@types/bull": "^3.15.0",
    "@types/dotenv": "^8.2.0",
    "@types/email-templates": "^7.1.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node-fetch": "^2.5.6",
    "@types/nodemailer": "^6.4.0",
    "@types/pug": "^2.0.4",
    "@types/yup": "0.29.11",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "apollo-server-testing": "^2.19.2",
    "eslint": "^7.20.0",
    "faker": "^5.4.0",
    "jest": "^26.6.3",
    "jest-date-mock": "^1.0.8",
    "ts-jest": "^26.4.4"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  }
}
